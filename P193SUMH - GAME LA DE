#include <iostream>
using namespace std;

long long n, m;

void Game(){
	int dem = 0;
	while(n != m){
		if(n < 0 || m < 0) dem = -1;
		else {
			if(n > m)
				dem = dem + n-m;
			if(n < m) {
				int k = m/n;
				dem = dem + k + m%n;
			}
		}
	}
	if(dem != -1) cout << dem << endl;
}

int main()
{
	cin >> n >> m;
	Game();
	return 0;
}
